<?xml version="1.0"?>

<!-- Lockheed Constellation 1049h Engineer Lever Hotspot panel -->
<!-- Gary R. Neely 'Buckaroo', 2008                            -->


<PropertyList>

<name>Engineer Lever Panel</name>
<background>Aircraft/Lockheed1049h/Panels/transparent-bg.rgb</background>
<w>256</w>
<h>8</h>

<instruments>

  <instrument>
    <name>panel hotspots</name>
    <x>128</x>
    <y>4</y>
    <w>256</w>
    <h>8</h>
    <w-base>256</w-base>
    <h-base>8</h-base>

    <actions>

<!-- RPM Lever -->

      <action>
        <name>RPMdec</name>
        <button>0</button>
        <x>-100</x>
        <y>-4</y>
        <w>10</w>
        <h>8</h>
        <binding>
          <command>property-adjust</command>
          <property>/controls/engines/engine[0]/propeller-pitch</property>
          <step>-0.02</step>
          <min>0</min>
          <max>1.0</max>
          <wrap>false</wrap>
        </binding>
        <binding>
          <command>property-adjust</command>
          <property>/controls/engines/engine[1]/propeller-pitch</property>
          <step>-0.02</step>
          <min>0</min>
          <max>1.0</max>
          <wrap>false</wrap>
        </binding>
        <binding>
