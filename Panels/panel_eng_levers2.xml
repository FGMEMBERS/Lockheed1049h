          <min>0</min>
          <max>1.0</max>
          <wrap>false</wrap>
        </binding>
        <binding>
          <command>property-adjust</command>
          <property>/controls/engines/engine[3]/propeller-pitch</property>
          <step>-0.02</step>
          <min>0</min>
          <max>1.0</max>
          <wrap>false</wrap>
        </binding>
      </action>
