<?xml version="1.0"?>

<!-- Lockheed Constellation 1049h Engineer Fuel Gauges Hotspot panel -->
<!-- Gary R. Neely 'Buckaroo', 2008                                  -->


<PropertyList>

  <name>Engineer Fuel Gauges Panel</name>
  <background>Panels/transparent-bg.rgb</background>
  <w>1024</w>
  <h>508</h>

  <instruments>

    <instrument>
      <name>panel hotspots</name>
      <x>512</x>
      <y>254</y>
      <w>1024</w>
      <h>508</h>
      <w-base>1024</w-base>
      <h-base>508</h-base>

      <actions>

        <!-- FuelLo +/- -->

        <action>
          <name>FuelLoDec</name>
          <button>0</button>
          <x>-470</x>
          <y>115</y>
          <w>50</w>
          <h>100</h>
          <binding>
            <command>property-adjust</command>
            <property>/consumables/fuel/tank[10]/level-gal_us</property>
            <step>-40</step>
            <min>0</min>
            <max>565.0</max>
            <wrap>false</wrap>
          </binding>
        </action>
        <action>
          <name>FuelLoInc</name>
