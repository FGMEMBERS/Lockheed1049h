<?xml version="1.0"?>

<PropertyList>

<fx>
    <!-- external -->
    <engine>
        <name>left wing</name>
        <path>/Aircraft/Lockheed1049h/Sounds/engine_running.wav</path>
        <mode>looped</mode>
        <condition>
          <and>
             <greater-than-equals>
               <property>engines/engine[0]/rpm</property>
              <value>800</value>
             </greater-than-equals>
             <not>
              <property>/sim/current-view/internal</property>
             </not>
          </and>
        </condition>
          <pitch>
          <property>engines/engine[0]/rpm</property>
          <factor>0.001</factor>
          <min>0.55</min>
          <max>1.0</max>
          <offset>0.3</offset>
         </pitch>
        <volume>
            <property>engines/engine[0]/rpm</property>
            <factor>0.0005</factor>
            <min>0.40</min>
            <max>1.5</max>
            <offset>0.065</offset>
        </volume>
        <position>
            <x>0</x>
            <y>3</y>
            <z>-3</z>
        </position>
        <reference-dist>50.0</reference-dist>
        <max-dist>3500.0</max-dist>
    </engine>

    <engine>
        <name>right wing</name>
        <path>/Aircraft/Lockheed1049h/Sounds/engine_running.wav</path>
        <mode>looped</mode>
        <condition>
          <and>
             <greater-than-equals>
               <property>engines/engine[2]/rpm</property>
              <value>800</value>
             </greater-than-equals>
             <not>
              <property>/sim/current-view/internal</property>
             </not>
          </and>
        </condition>
          <pitch>
          <property>engines/engine[2]/rpm</property>
          <factor>0.01</factor>
          <min>0.55</min>
          <max>1.0</max>
          <offset>0.3</offset>
         </pitch>
        <volume>
            <property>engines/engine[2]/rpm</property>
            <factor>0.0005</factor>
            <min>0.40</min>
            <max>1.5</max>
            <offset>0.065</offset>
        </volume>
        <position>
            <x>0</x>
            <y>-3</y>
            <z>-3</z>
        </position>
        <reference-dist>50.0</reference-dist>
        <max-dist>3500.0</max-dist>
    </engine>

    <engine>
        <name>left n1</name>
        <path>/Aircraft/Lockheed1049h/Sounds/engine_idle.wav</path>
        <mode>looped</mode>
        <condition>
          <and>
             <greater-than-equals>
               <property>engines/engine[0]/rpm</property>
              <value>50</value>
             </greater-than-equals>
             <less-than>
               <property>engines/engine[0]/rpm</property>
              <value>1800</value>
             </less-than>
          </and>
        </condition>
         <volume>
          <property>engines/engine[0]/rpm</property>
          <factor>0.01</factor>
          <min>1</min>
          <max>2</max>
          <offset>0.065</offset>
         </volume>
         <pitch>
          <property>/engines/engine[0]/rpm</property>
          <factor>0.001</factor>
          <min>0.55</min>
          <max>4.5</max>
          <offset>0.3</offset>
         </pitch>
        <position>
            <x>0</x>
            <y>-4</y>
            <z>-3</z>
        </position>
        <reference-dist>50.0</reference-dist>
        <max-dist>3500.0</max-dist>
    </engine>

    <engine>
        <name>right n1</name>
        <path>/Aircraft/Lockheed1049h/Sounds/engine_idle.wav</path>
        <mode>looped</mode>
        <condition>
          <and>
             <greater-than-equals>
               <property>engines/engine[2]/rpm</property>
              <value>50</value>
             </greater-than-equals>
             <less-than>
               <property>engines/engine[2]/rpm</property>
              <value>1800</value>
             </less-than>
          </and>
        </condition>
         <volume>
          <property>engines/engine[2]/rpm</property>
          <factor>0.001</factor>
          <min>0.05</min>
          <max>2</max>
          <offset>0.065</offset>
         </volume>
         <pitch>
          <property>/engines/engine[2]/rpm</property>
          <factor>0.01</factor>
          <min>1</min>
          <max>2</max>
          <offset>0.065</offset>
         </pitch>
        <position>
            <x>0</x>
            <y>-4</y>
            <z>-3</z>
        </position>
        <reference-dist>50.0</reference-dist>
        <max-dist>3500.0</max-dist>
    </engine>

<!-- internal -->
