<?xml version="1.0"?>

<PropertyList>

<path>fuel_pressure.ac</path>

<animation>
 <name>unused</name>
 <object-name>FuelPres.Needle1</object-name>
 <object-name>FuelPres.Needle2</object-name>
</animation>

<animation>
  <type>select</type>
  <object-name>unused</object-name>
  <condition>
    <equals><value>1</value><value>0</value></equals>
  </condition>
</animation>

<animation>
  <type>material</type>
  <object-name>FuelPres.Face</object-name>
  <object-name>FuelPres.Needle3</object-name>
  <object-name>FuelPres.Needle4</object-name>
  <emission>
    <red>0.8</red>
    <green>0.4</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>

<animation>
  <type>rotate</type>
  <object-name>FuelPres.Needle3</object-name>
  <property>/engines/engine[2]/est-fuelpress</property>
  <interpolation>
    <entry><ind> 0</ind><dep>  0</dep></entry>
    <entry><ind>35</ind><dep>315</dep></entry>
  </interpolation>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>FuelPres.Needle4</object-name>
  <property>/engines/engine[3]/est-fuelpress</property>
  <interpolation>
    <entry><ind> 0</ind><dep>  0</dep></entry>
    <entry><ind>35</ind><dep>315</dep></entry>
  </interpolation>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

</PropertyList>

