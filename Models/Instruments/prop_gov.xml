<PropertyList>

<path>prop_gov.ac</path>

<animation>
  <type>select</type>
  <object-name>LampPropGovOn1</object-name>
  <condition>
    <or>
      <greater-than-equals>
        <property>/engines/engine[0]/rpm</property>
        <value>2900.0</value>
      </greater-than-equals>
      <less-than>
        <property>/engines/engine[0]/rpm</property>
        <value>1350.0</value>
      </less-than>
    </or>
  </condition>
</animation>
<animation>
  <type>select</type>
  <object-name>LampPropGov1</object-name>
  <condition>
    <not>
    <or>
      <greater-than-equals>
