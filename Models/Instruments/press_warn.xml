<PropertyList>

<path>press_warn.ac</path>

<animation>
  <type>select</type>
  <object-name>LampPressFailOn1</object-name>
  <condition>
      <less-than-equals>
        <property>/engines/engine[0]/est-oilpress</property>
        <value>10.0</value>
      </less-than-equals>
  </condition>
</animation>
<animation>
  <type>select</type>
  <object-name>LampPressFail1</object-name>
  <condition>
    <not>
      <less-than-equals>
        <property>/engines/engine[0]/est-oilpress</property>
        <value>10.0</value>
      </less-than-equals>
    </not>
  </condition>
</animation>

