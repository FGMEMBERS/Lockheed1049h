<!--  Freq Transfer Button -->

<animation>
  <type>pick</type>
  <object-name>TFRbutton</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-swap</command>
      <property alias="../../../../params/selected-khz"/>
      <property alias="../../../../params/standby-khz"/>
    </binding>
  </action>
</animation>

</PropertyList>


