<?xml version="1.0"?>
<PropertyList>

<path>adf.ac</path>

<animation>
  <type>material</type>
  <object-name>Panel</object-name>
  <object-name>TFRbutton</object-name>
  <object-name>StdbyKnob1</object-name>
  <object-name>StdbyKnob2</object-name>
  <object-name>ActKnob1</object-name>
  <object-name>ActKnob2</object-name>
  <emission>
    <red>0.2</red>
    <green>0.1</green>
    <blue>0.05</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>
<animation>
  <type>material</type>
  <object-name>ActDigit1</object-name>
  <object-name>ActDigit2</object-name>
  <object-name>ActDigit3</object-name>
  <object-name>StdbyDigit1</object-name>
  <object-name>StdbyDigit2</object-name>
  <object-name>StdbyDigit3</object-name>
  <emission>
    <red>0.8</red>
    <green>0.4</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>

<!--  Active Freq Digits -->

<animation>
  <type>textranslate</type>
  <object-name>ActDigit1</object-name>
  <property alias="../../params/selected-khz"/>
  <factor>0.001</factor>
  <step>100</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>textranslate</type>
  <object-name>ActDigit2</object-name>
  <property alias="../../params/selected-khz"/>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>textranslate</type>
  <object-name>ActDigit3</object-name>
  <property alias="../../params/selected-khz"/>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation>

<!--  Active Freq Knobs -->

<animation>
  <type>rotate</type>
  <object-name>ActKnob1</object-name>
  <property alias="../../params/selected-khz"/>
  <interpolation>
    <entry><ind>190</ind><dep>  0</dep></entry>
    <entry><ind>535</ind><dep>720</dep></entry>
  </interpolation>
  <center>
    <x-m>0</x-m>
    <y-m>-0.038</y-m>
    <z-m>-0.013</z-m>
  </center>
  <axis>
    <x>1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>pick</type>
  <object-name>ActKnob1</object-name>
  <action>
    <button>0</button>
    <button>3</button>
    <repeatable>true</repeatable>
    <binding>
      <command>property-adjust</command>
      <property alias="../../../../params/selected-khz"/>
      <step>100</step>
      <min>190</min>
      <max>535</max>
      <wrap>true</wrap>
    </binding>
  </action>
  <action>
    <button>1</button>
    <button>4</button>
    <repeatable>true</repeatable>
    <binding>
      <command>property-adjust</command>
      <property alias="../../../../params/selected-khz"/>
      <step>-100</step>
      <min>190</min>
      <max>535</max>
      <wrap>true</wrap>
    </binding>
  </action>
</animation>
<animation>
  <type>rotate</type>
  <object-name>ActKnob2</object-name>
  <property alias="../../params/selected-khz"/>
  <interpolation>
    <entry><ind> 0</ind><dep>  0</dep></entry>
    <entry><ind>99</ind><dep>720</dep></entry>
  </interpolation>
  <center>
    <x-m>0</x-m>
    <y-m>-0.038</y-m>
    <z-m>-0.013</z-m>
  </center>
  <axis>
    <x>1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>
<animation>
  <type>pick</type>
  <object-name>ActKnob2</object-name>
  <action>
    <button>0</button>
    <button>3</button>
    <repeatable>true</repeatable>
    <binding>
      <command>property-adjust</command>
      <property alias="../../../../params/selected-khz"/>
      <step>1</step>
      <min>190</min>
      <max>535</max>
      <wrap>true</wrap>
    </binding>
  </action>
  <action>
    <button>1</button>
    <button>4</button>
    <repeatable>true</repeatable>
    <binding>
      <command>property-adjust</command>
      <property alias="../../../../params/selected-khz"/>
      <step>-1</step>
      <min>190</min>
      <max>535</max>
      <wrap>true</wrap>
    </binding>
  </action>
</animation>

