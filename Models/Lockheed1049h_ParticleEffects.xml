<!-- Smoke (starting engine, #1) -->
<model>
  <path>Aircraft/Lockheed1049h/Models/Effects/smoke.xml</path>
  <condition>
    <and>
      <property>engines/engine[0]/cranking</property>
      <property>engines/engine[0]/running</property>
    </and>
  </condition>
  <offsets>
    <x-m>-4.25</x-m>
    <y-m>-9.75</y-m>
    <z-m>0.9</z-m>
    <pitch-deg>-35</pitch-deg>
  </offsets>
</model>
