  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>RudderTab</object-name>
  <property>controls/flight/rudder-trim</property>
  <factor>-4680</factor>
  <center>
    <x-m>-15.228</x-m>
    <y-m>-0.055</y-m>
    <z-m>0.0</z-m>
  </center>
  <axis>
    <x>0.0</x>
    <y>0.0</y>
    <z>1.0</z>
  </axis>
</animation>
<animation>
  <type>pick</type>
  <object-name>RudderTab</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>/controls/flight/rudder-trim</property>
      <step>-0.01</step>
      <min>-1</min>
      <max>1</max>
      <wrap>false</wrap>
    </binding>
  </action>
</animation>
<animation>
  <type>pick</type>
  <object-name>RudderTab</object-name>
  <action>
    <button>1</button>
    <repeatable>false</repeatable>
