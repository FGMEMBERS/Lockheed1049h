    <y>0</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>RudderTabNeedle</object-name>
  <property>controls/flight/rudder-trim</property>
  <factor>60</factor>
  <center>
    <x-m>-15.263</x-m>
    <y-m>-0.054</y-m>
    <z-m>0.0</z-m>
  </center>
  <axis>
    <x>0.0</x>
    <y>0.0</y>
    <z>-1.0</z>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>AileronTabNeedle</object-name>
  <property>controls/flight/aileron-trim</property>
  <factor>60</factor>
  <center>
    <x-m>0.0</x-m>
    <y-m>-0.054</y-m>
    <z-m>0.596</z-m>
  </center>
  <axis>
    <x>1.0</x>
    <y>0.0</y>
