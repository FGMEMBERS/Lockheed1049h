<!-- Panel Glows -->

<animation>
  <type>material</type>
  <object-name>PedestalShelfText</object-name>  
  <condition>
		<not><property>/sim/rendering/rembrandt/enabled</property></not>
  </condition>
  <emission>
    <red>0.7</red>
    <green>0.2</green>
    <blue>0.1</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
