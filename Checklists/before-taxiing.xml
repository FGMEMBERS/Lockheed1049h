		<marker>
		    <x-m>-14.2463</x-m>
		    <y-m>1.0066</y-m>
		    <z-m>0.7793</z-m>
		    <scale>4.6529</scale>
		</marker>
		<condition>
		  <equals>
		    <property>/fdm/jsbsim/propulsion/engine[0]/boost-speed</property>
		    <value>0</value>
		  </equals>
		  <equals>
		    <property>/fdm/jsbsim/propulsion/engine[1]/boost-speed</property>
		    <value>0</value>
		  </equals>
		  <equals>
		    <property>/fdm/jsbsim/propulsion/engine[2]/boost-speed</property>
		    <value>0</value>
		  </equals>
		  <equals>
		    <property>/fdm/jsbsim/propulsion/engine[3]/boost-speed</property>
		    <value>0</value>
		  </equals>
		</condition>
  	  <binding>
          <command>property-assign</command>
        	<property>/fdm/jsbsim/propulsion/engine[0]/boost-speed</property>
        	<value>0</value>
        </binding>     
  	  <binding>
          <command>property-assign</command>
        	<property>/fdm/jsbsim/propulsion/engine[1]/boost-speed</property>
        	<value>0</value>
        </binding>     
  	  <binding>
          <command>property-assign</command>
        	<property>/fdm/jsbsim/propulsion/engine[2]/boost-speed</property>
        	<value>0</value>
        </binding>     
  	  <binding>
          <command>property-assign</command>
        	<property>/fdm/jsbsim/propulsion/engine[3]/boost-speed</property>
        	<value>0</value>
        </binding> 		   
	</item>
	
	<item>
		<name>MIXTURES</name>
		<value>RICH</value>
		<marker>
		    <x-m>-14.1215</x-m>
		    <y-m>0.9843</y-m>
		    <z-m>0.7793</z-m>
		    <scale>4.6529</scale>
		</marker>
		<condition>
		  <equals>
          <property>/controls/engines/engine[0]/mixture</property>
		    <value>1</value>
		  </equals>
		  <equals>
          <property>/controls/engines/engine[1]/mixture</property>
		    <value>1</value>
		  </equals>
		  <equals>
          <property>/controls/engines/engine[2]/mixture</property>
		    <value>1</value>
		  </equals>
		  <equals>
          <property>/controls/engines/engine[3]/mixture</property>
		    <value>1</value>
		  </equals>
		</condition>		
		<binding>
		  <command>property-assign</command>
          <property>/controls/engines/engine[0]/mixture</property>
	      <value>1</value>
		</binding> 
		<binding>
		  <command>property-assign</command>
          <property>/controls/engines/engine[1]/mixture</property>
	      <value>1</value>
		</binding>  
		<binding>
		  <command>property-assign</command>
          <property>/controls/engines/engine[2]/mixture</property>
	      <value>1</value>
		</binding>  
		<binding>
		  <command>property-assign</command>
          <property>/controls/engines/engine[3]/mixture</property>
	      <value>1</value>
		</binding> 
	</item>
	
    <item>
      <name>FUEL TANK SELECTORS</name>
      <value>TAKE OFF</value>
		<marker>
		    <x-m>-14.0050</x-m>
		    <y-m>0.9843</y-m>
		    <z-m>0.7793</z-m>
		    <scale>6.4876</scale>
		</marker>
      <condition>
		<and>
		    <equals>
		      <property>/controls/fuel/tankvalves[0]/valve</property>
		      <value>1</value>
		    </equals>
		    <equals>
		      <property>/controls/fuel/tankvalves[1]/valve</property>
		      <value>1</value>
		    </equals>
		    <equals>
		      <property>/controls/fuel/tankvalves[2]/valve</property>
		      <value>1</value>
		    </equals>
		    <equals>
		      <property>/controls/fuel/tankvalves[3]/valve</property>
		      <value>1</value>
		    </equals>
		</and>
      </condition>          
	  <binding>
        <command>property-assign</command>
          <property>/controls/fuel/tankvalves[0]/valve</property>
          <value>1</value>
      </binding>          
	  <binding>
        <command>property-assign</command>
          <property>/controls/fuel/tankvalves[1]/valve</property>
          <value>1</value>
      </binding>           
	  <binding>
        <command>property-assign</command>
          <property>/controls/fuel/tankvalves[2]/valve</property>
          <value>1</value>
      </binding>           
	  <binding>
        <command>property-assign</command>
          <property>/controls/fuel/tankvalves[3]/valve</property>
          <value>1</value>
      </binding>     
    </item>

	<item>
		<name>ALL TRIM WHEELS</name>
		<value>RESET</value>
		<condition>
			<and>
			  <equals>
			    <property>/controls/flight/elevator-trim</property>
			    <value>0</value>
			  </equals>
			  <equals>
			    <property>/controls/flight/rudder-trim</property>
			    <value>0</value>
			  </equals>
			  <equals>
			    <property>/controls/flight/aileron-trim</property>
			    <value>0</value>
			  </equals>
			</and>
		</condition>		
		<binding>
		  <command>nasal</command>
		  <script>Lockheed1049h.applyTrimWheels(0, 0);Lockheed1049h.applyTrimWheels(0, 1);Lockheed1049h.applyTrimWheels(0, 2);</script>
		</binding>       
	    <binding>
	      <command>property-toggle</command>
	      <property>/sim/sound/switch1</property>
	    </binding>
	</item>

	<item>
		<name>FLAPS</name>
		<value>EXTEND</value>
		<marker>
		    <x-m>-15.5719</x-m>
		    <y-m>0.1116</y-m>
		    <z-m>0.8793</z-m>
		    <scale>4.4876</scale>
		</marker>
		<condition>
		  <greater-than>
		    <property>controls/flight/flaps</property>
		    <value>0</value>
		  </greater-than>
		</condition>		
		<binding>
		  <command>nasal</command>
		  <script>controls.flapsDown(1)</script>
		</binding>    
	    <binding>
	      <command>property-toggle</command>
	      <property>/sim/sound/switch1</property>
	    </binding>
	</item>

	<item>
		<name>EXTERIOR/INTERIOR LIGHTS</name>
