  <item>
    <name>ALL TRIM WHEELS</name>
    <value>RESET</value>
    <condition>
      <and>
        <equals>
	  <property>controls/flight/elevator-trim</property>
	  <value>0</value>
        </equals>
        <equals>
	  <property>controls/flight/rudder-trim</property>
	  <value>0</value>
        </equals>
        <equals>
	  <property>controls/flight/aileron-trim</property>
	  <value>0</value>
        </equals>
      </and>
    </condition>		
    <binding>
      <command>nasal</command>
      <script>Lockheed1049h.applyTrimWheels(0, 0);Lockheed1049h.applyTrimWheels(0, 1);Lockheed1049h.applyTrimWheels(0, 2);</script>
    </binding>       
    <binding>
      <command>property-toggle</command>
      <property>sim/sound/switch1</property>
    </binding>
  </item>

  <item>
    <name>FLAPS</name>
    <value>EXTEND</value>
    <marker>
      <x-m>-15.5719</x-m>
      <y-m>0.1116</y-m>
      <z-m>0.8793</z-m>
      <scale>4.4876</scale>
    </marker>
    <condition>
      <greater-than>
        <property>controls/flight/flaps</property>
        <value>0</value>
      </greater-than>
    </condition>		
    <binding>
      <command>nasal</command>
      <script>controls.flapsDown(1)</script>
    </binding>    
    <binding>
      <command>property-toggle</command>
      <property>sim/sound/switch1</property>
    </binding>
  </item>

