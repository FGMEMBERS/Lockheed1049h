      <y-m>0.9818</y-m>
      <z-m>0.7793</z-m>
      <scale>4.6529</scale>
    </marker>
    <condition>
      <greater-than>
        <property>controls/engines/engine[0]/propeller-pitch</property>
        <value>0.94</value>
      </greater-than>
      <greater-than>
        <property>controls/engines/engine[1]/propeller-pitch</property>
        <value>0.94</value>
      </greater-than>
      <greater-than>
        <property>controls/engines/engine[2]/propeller-pitch</property>
        <value>0.94</value>
      </greater-than>
      <greater-than>
        <property>controls/engines/engine[3]/propeller-pitch</property>
        <value>0.94</value>
      </greater-than>
    </condition>
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[0]/propeller-pitch</property>
      <value>1</value>
    </binding>     
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[1]/propeller-pitch</property>
      <value>1</value>
    </binding>     
    <binding>
      <command>property-assign</command>
