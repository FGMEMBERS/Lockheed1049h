  <item>
    <name>FUEL TANK SELECTORS</name>
    <value>TAKE OFF</value>
    <marker>
      <x-m>-14.0050</x-m>
      <y-m>0.9843</y-m>
      <z-m>0.7793</z-m>
      <scale>6.4876</scale>
    </marker>
    <condition>
      <and>
        <equals>
	  <property>controls/fuel/tankvalves[0]/valve</property>
	  <value>1</value>
        </equals>
        <equals>
	  <property>controls/fuel/tankvalves[1]/valve</property>
	  <value>1</value>
        </equals>
        <equals>
	  <property>controls/fuel/tankvalves[2]/valve</property>
	  <value>1</value>
        </equals>
        <equals>
