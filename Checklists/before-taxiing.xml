<PropertyList> 
  <title>BEFORE TAXIING/TAKEOFF</title>    
  <item>
    <name>Engines startup</name>
    <value>COMPLETED</value>
  </item>

  <item>
    <name>COWL FLAPS</name>
    <value>1/2 OPEN</value>
    <marker>
      <x-m>-14.4818</x-m>
      <y-m>0.8190</y-m>
      <z-m>0.6198</z-m>
      <scale>4.6529</scale>
    </marker>
    <condition>
      <and>
        <greater-than>
	  <property>controls/engines/engine[0]/cowl-flaps-norm</property>
	  <value>0.2</value>
        </greater-than>
        <greater-than>
	  <property>controls/engines/engine[1]/cowl-flaps-norm</property>
	  <value>0.2</value>
        </greater-than>
        <greater-than>
	  <property>controls/engines/engine[2]/cowl-flaps-norm</property>
	  <value>0.2</value>
        </greater-than>
        <greater-than>
	  <property>controls/engines/engine[3]/cowl-flaps-norm</property>
	  <value>0.2</value>
        </greater-than>
      </and>
    </condition>      
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[0]/cowl-flaps-norm</property>
      <value>0.4</value>
    </binding>     
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[1]/cowl-flaps-norm</property>
      <value>0.4</value>
    </binding>     
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[2]/cowl-flaps-norm</property>
      <value>0.4</value>
    </binding>     
    <binding>
      <command>property-assign</command>
      <property>controls/engines/engine[3]/cowl-flaps-norm</property>
      <value>0.4</value>
    </binding>     
    <binding>
      <command>property-toggle</command>
      <property>sim/sound/switch1</property>
    </binding>    
  </item>

